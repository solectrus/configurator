    "vue": "^3.4.21",
    "vue-markdown-render": "^2.1.1"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@rushstack/eslint-patch": "^1.10.1",
    "@tailwindcss/typography": "^0.5.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
