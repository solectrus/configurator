    "eslint-plugin-vue": "^9.24.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "rollup": "^4.13.2",
    "rollup-plugin-gzip": "^3.1.2",
    "tailwindcss": "^3.4.3",
